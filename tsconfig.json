{
    "compilerOptions": {
        "lib": ["es2015"],
        "target": "es6",
        "module": "commonjs",
        "outDir": "dist",
        "rootDir": "./src",
        "strict": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "forceConsistentCasingInFileNames": true,
    },
    "include": ["src/**/*"],
}